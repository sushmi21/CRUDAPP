{
  "name": "crud-server",
  "version": "1.0.0",
  "description": "Express server for React",
  "main": "server.js",
  "scripts": {
    "client-install": "cd crud-client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix crud-client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sushmitha",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
